package com.sonata;

public class Product {
	int ProId;
	String ProName;
	double ProPrice;
	
	Product(){};
	Product(int a, String b, double c){
		this.ProId= a;
		this.ProName=b;
		this.ProPrice=c;
	}
	public double totalPrice(double p) {
		return this.ProPrice+p;
	}
	public void display() {
		System.out.println(ProId);
		System.out.println(ProName);
		System.out.println(ProPrice);
	}
}
