package com.sonata1;

public class Ass_Q6 {

	public static void main(String[] args) {
		String str= "Hello World";
		String h[] = str.split("");
		String Vow[] = {"a","e","i","o","u"};
		for(int i=0;i<h.length;i++){
		
			for(int j=0;j<Vow.length;j++){
			if(h[i] == Vow[j]) {
				System.out.print("Yes");
			}
		}
	}
		for(int j=0;j<h.length;j++){
		System.out.print(h[j]);
		}

  }

}
